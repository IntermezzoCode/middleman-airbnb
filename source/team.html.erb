
<div class="container">
  <h1>This template is made with <i class="fa fa-heart"></i> by </h1>
  <div class="row">
    <% data.users.each do |user| %>
    <div class="col-md-4 member">
     <%=  image_tag user.picture %>
     <h2><%= user.name %></h2>
     <p><i class="fa fa-github"></i><%= user.github_nickname %></p>
   </div>
   <% end %>
 </div>
</div>
